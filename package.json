{
  "name": "kings-league",
  "version": "1.0.0",
  "description": "API project for Kings League",
  "main": "index.js",
  "type": "module",
  "scripts": {
	"format": "prettier --write .",
	"lint:fix": "eslint . --ext .js,.astro,.ts,.tsx --fix",
	"lint": "eslint . --ext .js,.astro,.ts,.tsx",
    "astro": "astro",
    "build": "astro build",
    "dev:api": "wrangler dev",
    "dev": "astro dev",
    "preview": "astro preview",
    "publish:api": "wrangler publish",
    "scrape": "node --no-warnings scraping/index.js",
    "start": "astro dev",
    "test:cov": "vitest run --coverage",
    "test": "vitest"
  },
  "author": "√Ångel Barroso Sanz",
  "license": "ISC",
  "dependencies": {
    "@astrojs/tailwind": "^2.1.3",
    "astro": "1.9.0",
    "cheerio": "1.0.0-rc.12",
    "hono": "2.7.2",
    "picocolors": "^1.0.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint-plugin-astro": "^0.22.0",
    "prettier": "2.8.3",
    "prettier-plugin-astro": "^0.7.2",
    "standard": "17.0.0",
    "typescript": "^4.9.4",
    "vite": "4.0.4",
    "vitest": "0.27.0",
    "wrangler": "2.7.1"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:astro/recommended"
    ],
    "rules": {
      "space-before-function-paren": "off"
    }
  }
}
